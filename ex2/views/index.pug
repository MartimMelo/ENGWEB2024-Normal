extends layout

block content
    .w3-card-4
        header.w3-container.w3-teal
            h1= title                    
        .w3-container
                    table.w3-table-all
                        tr
                            th IdContrato 
                            th Objecto Contrato
                            th Data Celebração Contrato
                            th Preco Contratual
                            th NIPC Entidade Comunicante 
                            th Entidade Comunicante
                        each slist in lista
                            tr
                                td
                                    a(href='/' + slist._id) #{slist._id}
                                td= slist.objectoContrato
                                td= slist.dataCelebracaoContrato
                                td= slist.precoContratual
                                td
                                    a(href='/entidades/' + slist.NIPC_entidade_comunicante)= slist["NIPC_entidade_comunicante"]
                                td= slist.entidade_comunicante
        footer.w3-container.w3-blue 

extends layout

block content
    .w3-card-4
        header.w3-container.w3-teal
            h1 #{title} - #{entidade}   
            h2 Somatorio dos Preços Contratuais: #{sum}
        .w3-container
                    table.w3-table-all
                        tr
                            th IdContrato 
                            th Objecto Contrato
                            th Data Celebração Contrato
                            th Preco Contratual
                        each slist in lista
                            tr
                                td
                                    a(href='/' + slist._id) #{slist._id}
                                td= slist.objectoContrato
                                td= slist.dataCelebracaoContrato
                                td= slist.precoContratual

        footer.w3-container.w3-blue 
            address Generated by A100653 in 17/05/2024 - 
            a(href='/') Voltar
extends layout

block content
  .w3-card-4
    header.w3-container.w3-pink 
      h1 Contrato #{contrato._id}
    .w3-container
        ul.w3-ul.w3-card-4
            li Tipo de Procedimento: #{contrato.tipoprocedimento}
            li Objecto do Contrato: #{contrato.objectoContrato}
            li Data de Publicação: #{contrato.dataPublicacao}
            li Data de Celebração do Contrato: #{contrato.dataCelebracaoContrato}
            li Preço Contratual: #{contrato.precoContratual}
            li Prazo de Execução: #{contrato.prazoExecucao}
            li NIPC Entidade Comunicante: #{contrato.NIPC_entidade_comunicante}
            li Entidade Comunicante: #{contrato.entidade_comunicante}
            li Fundamentação: #{contrato.fundamentacao}

    footer.w3-container.w3-teal
        address Generated by A100653 in 16/05/2024 - 
            a(href='/') Voltar